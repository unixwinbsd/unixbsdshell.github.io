---
layout: null
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
<link rel="hub" href="https://pubsubhubbub.appspot.com" />
  <title>{{ site.title | xml_escape }}</title>
  <subtitle>{{ site.description }}</subtitle>
  <id>{{ site.url | absolute_url }}</id>
  <link type="application/atom+xml" href="{{ site.url }}/atom.xml" rel="self"/>
  <link href="{{ page.url | absolute_url }}" rel="alternate"/>
  <link href="{{site.url}}/" rel="alternate" type="text/html" />
  {% if site.copyright %}
     <rights type="text">{{ site.copyright }}</rights>
  {% endif %}  
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  {% for post in site.posts %}
  <entry>
    <title>{{ post.title }}</title>
    <link href="{{ post.url | absolute_url }}"/>
    <id>{{ site.url }}{{ post.id }}</id>
    <published>{{ post.date | date_to_xmlschema }}</published>
    <updated>{{ post.updated | date_to_xmlschema }}</updated>
    <author>
        <name>{{ site.author }}</name>
        <email>{{ site.email }}</email>
    </author>
      {% for category in post.categories %}
        <category term="{{ category | capitalize }}"/>
      {% endfor %}
      <summary type="html">{{ post.excerpt | xml_escape }}</summary>
      <content type="html">
        {{ post.content | xml_escape }}
      </content>



  </entry>
  {% endfor %}
</feed>